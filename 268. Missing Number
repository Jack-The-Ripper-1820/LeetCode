class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int n = nums.size();
        /*vector<bool> vis(n + 1);
        
        for(int &x : nums)
            vis[x] = true;
        
        for(int i = 0; i < n + 1; i++) {
            if(!vis[i])
                return i;
        }
        
        return -1;*/
        for(int i = 0; i < nums.size(); i++)
            n ^= (i ^ nums[i]); //xor
        return n;
    }
};
