class Solution {
public:
    string multiply(string num1, string num2) {
        vector<int> ans(num1.size() + num2.size());
        
        for(int i = num1.size() - 1; i >= 0; i--) {
            for(int j = num2.size() - 1; j >= 0; j--) {
                int a = ans[i + j + 1] + (num1[i] - '0') * (num2[j] - '0');
                int carry = a / 10;
                ans[i + j + 1] = a % 10;
                ans[i + j] += carry;
            }
        }
        
        int i = 0;
        string out;
        
        while(i < ans.size() && ans[i] == 0) i++;
        
        for(; i < ans.size(); i++)
            out += (char)(ans[i] + '0');
        
        return out == "" ? "0" : out;
    }
};
